{
    "name": "plients/xrel",
    "description": "A xREL.to Client for PHP",
    "keywords": [
        "api",
        "http",
        "xrel.to"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.1",
        "faustbrian/http": "^1.0",
        "somoza/oauth2-client-middleware": "^0.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Plients\\xREL\\": "src",
            "Plients\\Http\\": "HTTP/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plients\\Tests\\xREL\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
