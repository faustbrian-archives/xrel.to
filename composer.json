{
    "name": "faustbrian/xrel-php-client",
    "description": "A xREL.to Client for PHP",
    "keywords": ["api", "http", "xrel.to"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "faustbrian/http": "^1.0",
        "somoza/oauth2-client-middleware": "^0.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\xREL\\": "src",
            "BrianFaust\\Http\\": "HTTP/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\xREL\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
